          div
            a(href='https://koodiklinikka.slack.com')
              i.fa.fa-slack

            a(href='https://github.com/koodiklinikka/koodiklinikka.fi')
              i.fa.fa-github

            a(href='https://twitter.com/koodiklinikka')
